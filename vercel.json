{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    { "source": "/terms", "destination": "/tos", "permanent": true },
    { "source": "/privacy-policy", "destination": "/privacy", "permanent": true }
  ]
}
